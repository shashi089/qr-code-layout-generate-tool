{
  "name": "qr-layout-workspace",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:ui": "npm --workspace packages/ui run dev",
    "build:core": "npm --workspace packages/core run build",
    "build:ui": "npm --workspace packages/ui run build",
    "build": "npm run build:core && npm run build:ui"
  }
}
