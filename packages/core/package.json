{
  "name": "@qrlayout/core",
  "version": "1.0.0",
  "description": "QR sticker layout builder/renderer/exporter core",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./pdf": {
      "types": "./dist/pdf.d.ts",
      "import": "./dist/pdf.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json"
  },
  "sideEffects": false,
  "dependencies": {
    "qrcode": "^1.5.4"
  },
  "optionalDependencies": {
    "jspdf": "^3.0.4"
  },
  "devDependencies": {
    "@types/jspdf": "^1.3.3",
    "@types/qrcode": "^1.5.6",
    "typescript": "^5.3.3"
  }
}
