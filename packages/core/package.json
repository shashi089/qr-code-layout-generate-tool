{
  "name": "qrlayout-core",
  "version": "1.1.2",
  "description": "QR code layout builder, renderer, and exporter for stickers, labels, and PDFs",
  "keywords": [
    "qr",
    "qrcode",
    "qr-code",
    "qr-layout",
    "qr-layout",
    "qr-layout-designer",
    "layout",
    "qr-sticker",
    "qr-label",
    "zpl-generator",
    "pdf-generation",
    "label-printing",
    "layout",
    "qr-sticker",
    "qr-label",
    "generator",
    "renderer",
    "zpl",
    "thermal-printer"
  ],
  "author": "Shashidhar Naik <shashidharnaik8@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shashi089/qr-code-layout-generate-tool.git",
    "directory": "packages/core"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./pdf": {
      "types": "./dist/pdf.d.ts",
      "import": "./dist/pdf.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json"
  },
  "sideEffects": false,
  "dependencies": {
    "qrcode": "^1.5.4"
  },
  "optionalDependencies": {
    "jspdf": "^3.0.4"
  },
  "devDependencies": {
    "@types/qrcode": "^1.5.6",
    "typescript": "^5.3.3"
  }
}